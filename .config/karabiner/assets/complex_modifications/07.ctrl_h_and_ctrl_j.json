{
    "title": "07. ^h and ^j —— Backspace and Enter",
    "rules": [
        {
            "description": "07. ^h and ^j —— Backspace and Enter",
            "manipulators": [
                {
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": "left_control",
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": "left_control",
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "return_or_enter"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
