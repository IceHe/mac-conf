{
    "title": "18. Switch Profile",
    "rules": [
        {
            "description": "18. Profile  :  Fn + -  ——  IceHe  ,  Fn + =   ——  Default",
            "manipulators": [
                {
                    "from": {
                        "key_code": "hyphen",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'IceHe'"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "equal_sign",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'Default'"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
