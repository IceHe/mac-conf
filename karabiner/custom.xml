<?xml version="1.0"?>
<root>
    <item><appendix>Activate Layouts ------------------------</appendix></item>

    <item>
        <name>Hold 凵 → Layout 凵</name>
        <identifier>remap.space_plus</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::SPACE,
            KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_icehe_layout_space,
            KeyCode::SPACE
        </autogen>
    </item>

    <item>
        <name>Hold ⎋ → Layout Escape</name>
        <identifier>remap.escape_plus</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::ESCAPE,
            KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_icehe_layout_escape
        </autogen>
    </item>

    <item>
        <name>Hold R⌥ → Layout Links</name>
        <identifier>remap.option_r</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::OPTION_R,
            KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_icehe_layout_links
        </autogen>
    </item>

    <item><appendix>↩ ⌫ ← ↓ ↑ → --------------------</appendix></item>

    <item>
        <name>↩ ← ^ + J</name>
        <identifier>control_j</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::J, ModifierFlag::CONTROL_L | ModifierFlag::NONE,
            KeyCode::RETURN
        </autogen>
    </item>

    <item>
        <name>⌫ ← ^ + H</name>
        <identifier>control_h</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::H, ModifierFlag::CONTROL_L | ModifierFlag::NONE,
            KeyCode::DELETE
        </autogen>
    </item>

    <item>
        <name>↑ ← ^ + P</name>
        <identifier>control_p</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::P, ModifierFlag::CONTROL_L | ModifierFlag::NONE,
            KeyCode::CURSOR_UP
        </autogen>
    </item>

    <item>
        <name>↓ ← ^ + N</name>
        <identifier>control_n</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::N, ModifierFlag::CONTROL_L | ModifierFlag::NONE,
            KeyCode::CURSOR_DOWN
        </autogen>
    </item>

    <item>
        <name>→ ← ^ + F</name>
        <identifier>control_f</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::F, ModifierFlag::CONTROL_L | ModifierFlag::NONE,
            KeyCode::CURSOR_RIGHT
        </autogen>
    </item>

    <item>
        <name>← ← ^ + B</name>
        <identifier>control_b</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::B, ModifierFlag::CONTROL_L | ModifierFlag::NONE,
            KeyCode::CURSOR_LEFT
        </autogen>
    </item>

    <item><appendix>Input & ⎋ ------------------------</appendix></item>

    <item>
        <name>⎋</name>
        <appendix>EN ← Click ⎋</appendix>
        <identifier>remap.escape</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::ESCAPE,
            KeyCode::ESCAPE,
            KeyCode::F18
        </autogen>
    </item>

    <item>
        <name>⎋ ← R⌘</name>
        <appendix>EN ← Click R⌘</appendix>
        <identifier>remap.command_r_2</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::COMMAND_R,
            KeyCode::ESCAPE,
            KeyCode::F18
        </autogen>
    </item>


    <item>
        <name>L⇧</name>
        <appendix>EN ← Click L⇧</appendix>
        <identifier>remap.shift_l_2</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::SHIFT_L,
            KeyCode::SHIFT_L,
            KeyCode::F18
        </autogen>
    </item>

    <item>
        <name>R⇧</name>
        <appendix>EN ← Click R⇧</appendix>
        <identifier>remap.shift_r_2</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::SHIFT_R,
            KeyCode::SHIFT_R,
            KeyCode::F18
        </autogen>
    </item>

    <item>
        <name>CH ← F19</name>
        <appendix>Seil: F19 ← L^</appendix>
        <identifier>remap.f19_to_ch</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::F19,
            KeyCode::F17
        </autogen>
    </item>

    <item>
        <name>Switch Profiles</name>
        <identifier>remap.profiles</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::KEY_1, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L | ModifierFlag::OPTION_L | ModifierFlag::COMMAND_L,
            KeyCode::VK_OPEN_URL_APP_Karabiner_Profile0
        </autogen>
        <autogen>
            __KeyToKey__
            KeyCode::KEY_2, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L | ModifierFlag::OPTION_L | ModifierFlag::COMMAND_L,
            KeyCode::VK_OPEN_URL_APP_Karabiner_Profile1
        </autogen>
        <autogen>
            __KeyToKey__
            KeyCode::KEY_3, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L | ModifierFlag::OPTION_L | ModifierFlag::COMMAND_L,
            KeyCode::VK_OPEN_URL_APP_Karabiner_Profile2
        </autogen>
    </item>

</root>
