<?xml version="1.0"?>
<root>
    <item><appendix>HHKB JP ---------------------------</appendix></item>

    <item>
        <name>Hold 凵, Eisuu, Kana → Layout 凵</name>
        <identifier>hhkb_space_plus</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::SPACE,
            KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_icehe_layout_hhkb_space,
            KeyCode::SPACE
        </autogen>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::JIS_EISUU,
            KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_icehe_layout_hhkb_space,
            KeyCode::SPACE
        </autogen>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::JIS_KANA,
            KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_icehe_layout_hhkb_space,
            KeyCode::SPACE
        </autogen>
    </item>

    <item>
        <name>⎋ ← Fn + ⎋</name>
        <identifier>fn_escape</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::ESCAPE, ModifierFlag::FN | ModifierFlag::NONE,
            KeyCode::ESCAPE
        </autogen>
    </item>

    <item>
        <name>Fn ← F19</name>
        <appendix>Seil: F19 ← L^</appendix>
        <identifier>hhkb_f19</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::F19,
            KeyCode::FN
        </autogen>
    </item>

    <item>
        <name>CN ← `</name>
        <identifier>hhkb_backquote</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::BACKQUOTE,
            KeyCode::F17
        </autogen>
    </item>

    <item>
        <name>⌫ ← ￥</name>
        <identifier>hhkb_yen</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::JIS_YEN,
            KeyCode::DELETE
        </autogen>
    </item>

    <item>
        <name>R⇧ ← JIS_UNDERSCORE & ↑</name>
        <appendix></appendix>
        <identifier>hhkb_jis_underscore</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::JIS_UNDERSCORE,
            KeyCode::SHIFT_R,
            KeyCode::F18
        </autogen>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::CURSOR_UP,
            KeyCode::SHIFT_R,
            KeyCode::F18
        </autogen>
    </item>

    <item>
        <name>⇪ ← ←</name>
        <identifier>hhkb_left</identifier>
        <autogen>
            __KeyToKey__
            KeyCode::CURSOR_LEFT,
            KeyCode::CAPSLOCK
        </autogen>
    </item>
</root>
